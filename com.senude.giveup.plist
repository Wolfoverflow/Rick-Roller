<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.senude.giveup</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/nohup</string>
    <string>/bin/bash</string>
    <string>-c</string>
    <string>while true; do osascript -e "set volume output volume 100"; open https://senude.github.io/rickroll.mp4; sleep 300; done</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>
